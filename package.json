{
  "name": "PWAscore",
  "private": true,
  "type": "module",
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "lint-staged": {
    "*.{js,mjs,ts,vue}": [
      "eslint --fix"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "build": "node scripts/generate-og-screenshot.mjs && nuxt build",
    "dev": "nuxt dev",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "nuxt typecheck",
    "precommit": "pnpm run lint && pnpm run typecheck",
    "fix": "pnpm run lint:fix",
    "update-caniuse": "node scripts/download-caniuse.mjs",
    "generate-og": "node scripts/generate-og-screenshot.mjs",
    "deploy": "pnpm build && wrangler deploy",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky"
  },
  "dependencies": {
    "@iconify-json/lucide": "^1.2.69",
    "@iconify-json/simple-icons": "^1.2.54",
    "@nuxt/content": "^3.7.1",
    "@nuxt/image": "^1.11.0",
    "@nuxt/ui": "^4.0.1",
    "nuxt": "^4.1.3",
    "valibot": "^1.1.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251011.0",
    "@iconify-json/heroicons": "^1.2.3",
    "@mdn/browser-compat-data": "^7.1.12",
    "@nuxt/eslint": "^1.9.0",
    "@nuxt/test-utils": "^3.19.2",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.37.0",
    "happy-dom": "^20.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.4",
    "puppeteer-core": "^24.24.1",
    "sharp": "^0.34.4",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.1.1",
    "wrangler": "^4.42.2"
  },
  "resolutions": {
    "unimport": "4.1.1"
  },
  "packageManager": "pnpm@10.18.0"
}
